class Solution {
public:
    bool canVisitAllRooms(vector<vector<int>>& rooms) {
        vector<bool> done(rooms.size(),false);
        queue<int> keys;
        done[0]=true;
        int count=1;
        for (int a :rooms[0]){
            keys.push(a);
        }
        while (!keys.empty()){
            int i=keys.front();keys.pop();
            
        if (!done[i]){
            for (int a :rooms[i]){
                keys.push(a);
        }
            done[i]=true;
            count++;
        }
        
        }
        if(count==rooms.size()) return true;
        else return false;
    }
};
