class Solution {
public:


    int evalRPN(vector<string>& tokens) {
        stack<long long> s;long long x;
        for (int i=0;i<tokens.size();++i){
            if(tokens[i].size()==1 && ! isdigit(tokens[i][0])){
                long long a=s.top();s.pop();
                long long b=s.top();s.pop();
                switch(tokens[i][0]){
                    case '+':  x=a+b;s.push(x);break;
                    case '-':  x=b-a;s.push(x);break;
                    case '*':  x=b*a;s.push(x);break;
                    case '/':  x=b/a;s.push(x);break;
                }


            }
            else s.push(stoi(tokens[i],0,10));
        }
        if (s.empty()) return 0;
        else return s.top();
    }
};
