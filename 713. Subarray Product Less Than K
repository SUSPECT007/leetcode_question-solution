class Solution {
public:
    int numSubarrayProductLessThanK(vector<int>& nums, int k) {
        if(k<=1) return 0;
        int n=nums.size(),count=0,r=0,l=0,product=1;
        while(r<n){
            product*=nums[r];
            while(product>=k) product/=nums[l++];
            count+=r-l+1;
            ++r;
        }
        return count;
    }
};
