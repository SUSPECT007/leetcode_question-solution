class Solution {
public:
    string maximumOddBinaryNumber(string s) {
        string ans;
        int n=s.size(),n1=0;
        for(char ch:s){
            if(ch=='1') ++n1;
        }
        string str1(n1-1,'1'),str2(n-n1,'0');

        ans=str1+str2;
        ans+='1';
        return ans;

    }
};
