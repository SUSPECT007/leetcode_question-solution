class Solution {
public:
    int maxSatisfaction(vector<int>& satisfaction) {
        sort(satisfaction.begin(),satisfaction.end());
        int n=satisfaction.size();
        int ans=0,pre=0;
        for(int i=n-1;i>=0;--i){
            if(pre+satisfaction[i]>0){
                pre+=satisfaction[i];
                ans+=pre;

            }
            else break;
        }
        return ans;
    }
};
