class Solution {
public:
    string largestGoodInteger(string num) {
        int count=0;
        char c='0';
        string ans="";
        for(int i=1;i<num.size();++i){
            if (num[i]==num[i-1]) ++count;
            else {count=0;continue;}
            if(count==2&&num[i]>=c){ans="";c=num[i];ans+=c;ans+=c;ans+=c;}
        }
        return ans;

    }
};
